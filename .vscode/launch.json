{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}/vscode_extension"],
      "outFiles": ["${workspaceFolder}/vscode_extension/dist/**/*.js"],
      "preLaunchTask": "npm: build"
    }
  ]
}
